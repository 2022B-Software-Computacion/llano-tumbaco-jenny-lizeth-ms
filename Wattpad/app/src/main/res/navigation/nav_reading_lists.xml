<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_reading_lists"
    app:startDestination="@id/ReadingFragment">

    <fragment
        android:id="@+id/ReadingFragment"
        android:name="com.example.recyclerviewlibro.fragments.ReadingLists"
        tools:layout="@layout/fragment_reading_lists">

        <action
            android:id="@+id/action_ReadingFragment_to_LibrosFragment"
            app:destination="@id/LibrosFragment" />
    </fragment>
    <fragment
        android:id="@+id/LibrosFragment"
        android:name="com.example.recyclerviewlibro.fragments.LibrosRecyclerView"
        tools:layout="@layout/fragment_libro">

    </fragment>
</navigation>